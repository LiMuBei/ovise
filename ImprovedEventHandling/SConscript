# -*- python -*-
Import('MODE', 'env')
LocalEnv = env.Clone()

### Setting up environment
if MODE == 'debug':
    LocalEnv.MergeFlags({'CCFLAGS':['-g','-Wall']})

## Input files
input_files = Glob('*.cpp')

if MODE == 'debug':
    env.Library(target='#lib/Debug/ImprovedEventHandling', source=input_files)
