            <xs:enumeration value="PF_L16" />
            <xs:enumeration value="PF_SHORT_L" />
            <xs:enumeration value="PF_A8" />
            <xs:enumeration value="PF_BYTE_A" />
            <xs:enumeration value="PF_A4L4" />
            <xs:enumeration value="PF_BYTE_LA" />
            <xs:enumeration value="PF_R5G6B5" />
            <xs:enumeration value="PF_B5G6R5" />
            <xs:enumeration value="PF_R3G3B2" />
            <xs:enumeration value="PF_A4R4G4B4" />
            <xs:enumeration value="PF_A1R5G5B5" />
            <xs:enumeration value="PF_R8G8B8" />
            <xs:enumeration value="PF_B8G8R8" />
            <xs:enumeration value="PF_A8R8G8B8" />
            <xs:enumeration value="PF_A8B8G8R8" />
            <xs:enumeration value="PF_B8G8R8A8" />
            <xs:enumeration value="PF_R8G8B8A8" />
            <xs:enumeration value="PF_X8R8G8B8" />
            <xs:enumeration value="PF_X8B8G8R8" />
            <xs:enumeration value="PF_BYTE_RGB" />
            <xs:enumeration value="PF_BYTE_BGR" />
            <xs:enumeration value="PF_BYTE_BGRA" />
            <xs:enumeration value="PF_BYTE_RGBA" />
            <xs:enumeration value="PF_A2R10G10B10" />
            <xs:enumeration value="PF_A2B10G10R10" />
            <xs:enumeration value="PF_DXT1" />
            <xs:enumeration value="PF_DXT2" />
            <xs:enumeration value="PF_DXT3" />
            <xs:enumeration value="PF_DXT4" />
            <xs:enumeration value="PF_DXT5" />
            <xs:enumeration value="PF_FLOAT16_R" />
            <xs:enumeration value="PF_FLOAT16_RGB" />
            <xs:enumeration value="PF_FLOAT16_RGBA" />
            <xs:enumeration value="PF_FLOAT32_R" />
            <xs:enumeration value="PF_FLOAT32_RGB" />
            <xs:enumeration value="PF_FLOAT32_RGBA" />
            <xs:enumeration value="PF_FLOAT16_GR" />
            <xs:enumeration value="PF_FLOAT32_GR" />
            <xs:enumeration value="PF_DEPTH" />
            <xs:enumeration value="PF_SHORT_RGBA" />
            <xs:enumeration value="PF_SHORT_GR" />
            <xs:enumeration value="PF_SHORT_RGB" />
            <xs:enumeration value="PF_COUNT" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="0.6" name="shadowDirLightTextureOffset" type="xs:string" />
      <xs:attribute default="0.7" name="shadowTextureFadeStart" type="xs:string" />
      <xs:attribute default="0.9" name="shadowTextureFadeEnd" type="xs:string" />
      <xs:attribute name="shadowTextureSelfShadow">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="shadowTextureCasterMaterial" type="xs:string" />
      <xs:attribute name="shadowTextureReceiverMaterial" type="xs:string" />
      <xs:attribute default="true" name="shadowCasterRenderBackFaces">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="shadowTextureConfig">
    <xs:complexType>
      <xs:attribute name="shadowIndex" type="xs:string" use="required" />
      <xs:attribute name="width" type="xs:string" use="required" />
      <xs:attribute name="height" type="xs:string" use="required" />
      <xs:attribute name="pixelFormat" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="PF_UNKNOWN" />
            <xs:enumeration value="PF_L8" />
            <xs:enumeration value="PF_BYTE_L" />
            <xs:enumeration value="PF_L16" />
            <xs:enumeration value="PF_SHORT_L" />
            <xs:enumeration value="PF_A8" />
            <xs:enumeration value="PF_BYTE_A" />
            <xs:enumeration value="PF_A4L4" />
            <xs:enumeration value="PF_BYTE_LA" />
            <xs:enumeration value="PF_R5G6B5" />
            <xs:enumeration value="PF_B5G6R5" />
