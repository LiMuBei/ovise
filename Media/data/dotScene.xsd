  <xs:element name="resourceGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="resourceLocation" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="resourceDeclaration" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="resourceLocation">
    <xs:complexType>
      <xs:attribute name="name" type="xs:string" use="required" />
      <xs:attribute name="type" type="xs:string" use="required" />
      <xs:attribute default="true" name="recursive">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="dotSceneFileLocation" name="relativeTo">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="dotSceneFileLocation" />
            <xs:enumeration value="exeFileLocation" />
            <xs:enumeration value="currentWorkingDir" />
            <xs:enumeration value="absolute" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="resourceDeclaration">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="NameValuePairList" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" use="required" />
      <xs:attribute name="resourceType" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="NameValuePairList">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="NameValuePair" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NameValuePair">
    <xs:complexType>
      <xs:attribute name="name" type="xs:string" use="required" />
      <xs:attribute name="value" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="terrain">
    <xs:complexType>
      <xs:attribute name="dataFile" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="nodes">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="node" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="light" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="camera" />
        <xs:element minOccurs="0" maxOccurs="1" ref="position" />
        <xs:element minOccurs="0" maxOccurs="1" ref="orientation" />
        <!--(#5)-->
        <xs:element minOccurs="0" maxOccurs="1" ref="quaternion" /> 
        <xs:element minOccurs="0" maxOccurs="1" ref="scale" />
      </xs:sequence>
      <xs:attribute default="true" name="visible">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="true" name="cascadeVisibility">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="node">
    <xs:complexType>
      <xs:sequence>
        <!--(#8)-->
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element minOccurs="0" maxOccurs="1" ref="position" />
          <xs:element minOccurs="0" maxOccurs="1" ref="orientation" />
          <!--(#5)-->
          <xs:element minOccurs="0" maxOccurs="1" ref="quaternion" />
          <xs:element minOccurs="0" maxOccurs="1" ref="scale" />
          <xs:element minOccurs="0" maxOccurs="1" ref="lookTarget" />
          <xs:element minOccurs="0" maxOccurs="1" ref="trackTarget" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="node" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="entity" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="light" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="camera" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="particleSystem" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="billboardSet" />
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="plane" />
          <xs:element minOccurs="0" maxOccurs="1" ref="userDataReference" />
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" />
      <xs:attribute name="id" type="xs:ID" />
      <xs:attribute default="true" name="visible">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="true" name="cascadeVisibility">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="false" name="isTarget">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="particleSystem">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="commonMovableObjectParams" />
        <xs:element minOccurs="0" maxOccurs="1" ref="userDataReference" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" />
      <xs:attribute name="id" type="xs:ID" />
      <xs:attribute name="templateName" type="xs:string" use="required" />
      <xs:attribute name="materialName" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="light">
    <xs:complexType>
      <xs:sequence>
        <!--(#12)-->
        <xs:element minOccurs="0" maxOccurs="1" ref="normal" />
        <xs:element minOccurs="0" maxOccurs="1" ref="commonMovableObjectParams" />
        <xs:element minOccurs="0" maxOccurs="1" ref="position" />
        <xs:element minOccurs="0" maxOccurs="1" ref="direction" />
        <xs:element minOccurs="0" maxOccurs="1" ref="colourDiffuse" />
        <xs:element minOccurs="0" maxOccurs="1" ref="colourSpecular" />
        <!--(#11)-->
        <xs:element minOccurs="0" maxOccurs="1" ref="lightRange" />
        <xs:element minOccurs="0" maxOccurs="1" ref="lightAttenuation" />
        <xs:element minOccurs="0" maxOccurs="1" ref="userDataReference" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" />
      <xs:attribute name="id" type="xs:ID" />
      <xs:attribute default="point" name="type">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="point" />
            <xs:enumeration value="directional" />
            <xs:enumeration value="spotLight" />
            <xs:enumeration value="radPoint" />
          </xs:restriction>
        </xs:simpleType>
       
      </xs:attribute>
      <xs:attribute default="1.0" name="powerScale" type="xs:string" />
      <!--(#10)-->
      <xs:attribute default="false" name="castShadows">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="camera">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="commonMovableObjectParams" />
        <xs:element minOccurs="0" maxOccurs="1" ref="clipping" />
        <xs:element minOccurs="0" maxOccurs="1" ref="position" />
        <xs:element minOccurs="0" maxOccurs="1" ref="orientation" />
        <!--(#5)-->
        <xs:element minOccurs="0" maxOccurs="1" ref="quaternion" />
        <xs:element minOccurs="0" maxOccurs="1" ref="lookTarget" />
        <xs:element minOccurs="0" maxOccurs="1" ref="trackTarget" />
        <xs:element minOccurs="0" maxOccurs="1" ref="userDataReference" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" />
      <xs:attribute name="id" type="xs:ID" />
      <!--(#9)-->
