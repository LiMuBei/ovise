            <xs:enumeration value="stencilModulative" />
            <xs:enumeration value="stencilAdditive" />
            <xs:enumeration value="textureModulative" />
            <xs:enumeration value="textureAdditive" />
            <xs:enumeration value="textureModulativeIntegrated" />
            <xs:enumeration value="textureAdditiveIntegrated" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="false" name="showDebugShadows">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="shadowFarDistance" type="xs:string" />
      <xs:attribute default="10000" name="shadowDirectionalLightExtrusionDistance" type="xs:string" />
      <xs:attribute default="51200" name="shadowIndexBufferSize" type="xs:string" />
      <xs:attribute default="true" name="shadowUseInfiniteFarPlane">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="true" />
            <xs:enumeration value="false" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="shadowTextureCount" type="xs:string" />
      <xs:attribute name="shadowTextureSize" type="xs:string" />
      <xs:attribute name="shadowTexturePixelFormat">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="PF_UNKNOWN" />
